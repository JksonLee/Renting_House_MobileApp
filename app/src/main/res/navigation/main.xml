<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.lifeafterdom_assignment.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_self"
            app:destination="@id/loginFragment" />
        <argument
            android:name="userID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.lifeafterdom_assignment.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.lifeafterdom_assignment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_roomDetailFragment"
            app:destination="@id/roomDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_favoredFragment"
            app:destination="@id/favoredFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profilePageFragment"
            app:destination="@id/profilePageFragment" />
        <action
            android:id="@+id/action_homeFragment_self"
            app:destination="@id/homeFragment" />
        <argument
            android:name="userID"
            app:argType="integer" />
        <action
            android:id="@+id/action_homeFragment_to_propertyPageFragment"
            app:destination="@id/propertyPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoredFragment"
        android:name="com.example.lifeafterdom_assignment.FavoredFragment"
        android:label="fragment_favored"
        tools:layout="@layout/fragment_favored" >
        <action
            android:id="@+id/action_favoredFragment_to_roomDetailFragment"
            app:destination="@id/roomDetailFragment" />
        <action
            android:id="@+id/action_favoredFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_favoredFragment_self"
            app:destination="@id/favoredFragment" />
        <action
            android:id="@+id/action_favoredFragment_to_profilePageFragment"
            app:destination="@id/profilePageFragment" />
        <argument
            android:name="userID"
            app:argType="integer" />
        <action
            android:id="@+id/action_favoredFragment_to_propertyPageFragment"
            app:destination="@id/propertyPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/profilePageFragment"
        android:name="com.example.lifeafterdom_assignment.ProfilePageFragment"
        android:label="fragment_profile_page"
        tools:layout="@layout/fragment_profile_page" >
        <action
            android:id="@+id/action_profilePageFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_profilePageFragment_self"
            app:destination="@id/profilePageFragment" />
        <action
            android:id="@+id/action_profilePageFragment_to_favoredFragment"
            app:destination="@id/favoredFragment" />
        <argument
            android:name="userID"
            app:argType="integer" />
        <action
            android:id="@+id/action_profilePageFragment_to_propertyPageFragment"
            app:destination="@id/propertyPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/roomDetailFragment"
        android:name="com.example.lifeafterdom_assignment.RoomDetailFragment"
        android:label="fragment_room_detail"
        tools:layout="@layout/fragment_room_detail" >
        <action
            android:id="@+id/action_roomDetailFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="roomSelectedItem"
            app:argType="integer" />
        <argument
            android:name="userID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/addNewRoomFragment"
        android:name="com.example.lifeafterdom_assignment.AddNewRoomFragment"
        android:label="fragment_add_new_room"
        tools:layout="@layout/fragment_add_new_room" >
        <action
            android:id="@+id/action_addNewRoomFragment_to_propertyPageFragment"
            app:destination="@id/propertyPageFragment" />
        <argument
            android:name="userID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/propertyPageFragment"
        android:name="com.example.lifeafterdom_assignment.PropertyPageFragment"
        android:label="fragment_property_page"
        tools:layout="@layout/fragment_property_page" >
        <action
            android:id="@+id/action_propertyPageFragment_to_addNewRoomFragment"
            app:destination="@id/addNewRoomFragment" />
        <action
            android:id="@+id/action_propertyPageFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_propertyPageFragment_to_profilePageFragment"
            app:destination="@id/profilePageFragment" />
        <action
            android:id="@+id/action_propertyPageFragment_to_favoredFragment"
            app:destination="@id/favoredFragment" />
        <action
            android:id="@+id/action_propertyPageFragment_self"
            app:destination="@id/propertyPageFragment" />
        <action
            android:id="@+id/action_propertyPageFragment_to_editPropertyFragment"
            app:destination="@id/editPropertyFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/propertyPageFragment" />
        <argument
            android:name="userID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/editPropertyFragment"
        android:name="com.example.lifeafterdom_assignment.EditPropertyFragment"
        android:label="fragment_edit_property"
        tools:layout="@layout/fragment_edit_property" >
        <argument
            android:name="roomSelectedItem"
            app:argType="integer" />
        <argument
            android:name="userID"
            app:argType="integer" />
        <action
            android:id="@+id/action_editPropertyFragment_to_propertyPageFragment"
            app:destination="@id/propertyPageFragment" />
    </fragment>
</navigation>